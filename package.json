{
    "name": "vite-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "dev": "vite --host --port 5173",
        "build": "tsc && vite build",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --color --fix --ignore-pattern src/routeTree.gen.ts",
        "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx,md,scss,css,html}\" --ignore-path .prettierignore",
