        "globals": "16.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^16.1.5",
        "postcss": "8.5.6",
        "prettier": "3.5.3",
        "sass-embedded": "^1.90.0",
        "typescript": "5.6.3",
        "vite": "6.0.11",
        "vite-tsconfig-paths": "5.1.4"
    },
    "lint-staged": {
        "src/**/*.{json,ts,tsx,js,jsx,md,scss,css,html}": [
            "npm run format"
        ],
        "src/**/*.{ts,tsx,js,jsx}": [
            "npm run lint"
        ],
        "src/routeTree.gen.ts": "echo 'skip routeTree.gen.ts'"
    }
}