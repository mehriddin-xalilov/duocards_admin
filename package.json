        "@tanstack/react-router-devtools": "^1.130.13",
        "@tanstack/router-plugin": "^1.130.15",
        "@types/lodash.throttle": "^4.1.9",
        "@types/node": "20.5.7",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
